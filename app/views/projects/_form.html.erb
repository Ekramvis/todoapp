<%= form_for(@project) do |f| %>
	<%= f.hidden_field :team_id %>
	
	<%= f.label :team_id %>
	<%= f.collection_select :team_id, Team.all, :id, :name %>
	<br>
	<%= f.label :title %>
	<%= f.text_field :title %> 
	<br>

	<%= f.label :description %>
	<br>
	<%= f.text_area :description %>
	<br>

	<%= f.submit "Save Project" %>
<% end %>